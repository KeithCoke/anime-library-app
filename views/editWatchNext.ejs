<head>
    <%- include ('./partials/head.ejs') %>
</head>

<body>
    <a href="/"><button>Home</button></a>
    <div id="flex-container">
        <form action="/watchNext/<%= watchNext._id %>?_method=PUT" method="post">
            <label for="title">Title:</label>
            <input type="text" name="title" value="<%= watchNext.title %>" />
            <label for="description">Description:</label>
            <textarea name="description" ><%= watchNext.description %></textarea>
            <button type="submit">Edit watchNext</button>
        </form>
    </div>
</body>

</html>